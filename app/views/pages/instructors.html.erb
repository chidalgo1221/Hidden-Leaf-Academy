<header>
        <center>
        <img src="https://i.pinimg.com/originals/a4/f8/f6/a4f8f61b20234ca9323dab3e0091ec18.png" width="10%">
        </center>
        <ul class="navs">
            <li><a href="/home" class="link">Home</a></li>
            <li><a href="/courses" class="link">Courses</a></li>
            <li><a href="/instructors" class="link">Instructors</a></li>
            <li><a href="/students" class="link">Students</a></li>
            <a class="btn btn-primary" href="logout">Logout</a>

         </ul>
    </header>
    <br>
<table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Education Level</th>
        <th scope="col">Birthday</th>
        <th scope="col">Salary</th>

      </tr>
    </thead>
    <tbody>
    <% @instructors.each do |instructors| %>
      <tr>
        <td><%=instructors.first_name%></td>
        <td><%=instructors.last_name%></td>
        <td><%=instructors.education_level%></td>
        <td><%=instructors.age%></td>
        <td><%=instructors.salary%></td>
      </tr>
    <%end%>
    </tbody>
  </table>

<%= form_tag("/instructors", method: "POST") do %>
    <%= text_field(:instructor, :first_name) %>
    <%= text_field(:instructor, :last_name) %>
    <%= text_field(:instructor, :education_level) %>
    <%= date_field(:instructor, :age) %>
    <%= text_field(:instructor, :salary) %>
    <%= submit_tag("Add Instructor") %>
  <% end %>